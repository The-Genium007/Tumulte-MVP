apiVersion: 1

datasources:
  # Prometheus - Source principale
  - name: Prometheus
    uid: prometheus
    type: prometheus
    access: proxy
    url: http://tumulte-prometheus:9090
    isDefault: true
    editable: false
    jsonData:
      timeInterval: '15s'
      exemplarTraceIdDestinations: []
